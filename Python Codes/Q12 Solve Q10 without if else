time1 = input("Enter the first lap time in mm:ss:msms format, without the colons: ")
time2 = input("Enter the second lap time in mm:ss:msms format, without the colons: ")
time3 = input("Enter the third lap time in mm:ss:msms format, without the colons: ")

a = int(time1)
b = int(time2)
c = int(time3)

temp = a if a < b else b
result = c if c < temp else temp

x = str(result)

mm = x[0:2]
ss = x[2:4]
msms = x[4:6]

print("The fastest lap time is ", mm, ":", ss, ":", msms, ".")
