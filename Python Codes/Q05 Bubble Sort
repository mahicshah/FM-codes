arr = []
number = int(input("Enter the number of elements: "))

for i in range(number):
    value = int(input("Enter the %d element of the list : " %i))
    arr.append(value)

for i in range(number -1):
    for j in range(number - i - 1):
        if(arr[j] > arr[j + 1]):
             temp = arr[j]
             arr[j] = arr[j + 1]
             arr[j + 1] = temp

print("The sorted list is: ", arr)
